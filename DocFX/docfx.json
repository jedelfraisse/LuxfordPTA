{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "events/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "documents/**",
          "styles/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appLogoPath": "images/pta-logo-45.jpg",
      "_appName": "Luxford Elementary PTA",
      "_appTitle": "Luxford Elementary PTA",
      "_extraCss": [
        "styles/main.css"
      ],
      "_disableContribution": true
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}